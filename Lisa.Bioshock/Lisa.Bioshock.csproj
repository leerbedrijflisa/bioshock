  <!-- Define files to be copied when compiling here -->
  <Target Name="BeforeBuild" Condition="'@(BuildBasedFiles)' != ''" Outputs="%(BuildBasedFiles.Identity)">
    <!-- Generate new file name and destination folder -->
    <PropertyGroup>
      <!-- Get the directory name -->
      <OriginalFileFolder>$([System.IO.Path]::GetDirectoryName(%(BuildBasedFiles.FullPath)))</OriginalFileFolder>
      <!-- Get the file name -->
      <OriginalFileName>%(BuildBasedFiles.FileName)%(BuildBasedFiles.Extension)</OriginalFileName>
      <!-- Generate the suffix to replace (Debug or Release) -->
      <Suffix>.$(Configuration)</Suffix>
      <!-- Generate the destination file path -->
      <DestinationFile>$(OriginalFileFolder)\$(OriginalFileName.Replace('$(Suffix)', ''))</DestinationFile>
    </PropertyGroup>
    <!-- Log message to output window -->
    <Message Text="Copy '$(OriginalFileName)' to '$(DestinationFile)'" Importance="high" />
    <!-- Copy source file(s) to the generated destination directory -->
    <Copy SourceFiles="%(BuildBasedFiles.FullPath)" DestinationFiles="$(DestinationFile)" />
  </Target>
